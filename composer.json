{
    "name": "gurgentil/laravel-eloquent-sequencer",
    "description": "A package that allows you to create and manage sequences on Eloquent models.",
    "keywords": [
        "gurgentil",
        "laravel",
        "eloquent",
        "order",
        "sequence"
    ],
    "homepage": "https://github.com/gurgentil/laravel-eloquent-sequencer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gustavo Rorato Gentil",
            "email": "gustavorgentil@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Gurgentil\\LaravelEloquentSequencer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gurgentil\\LaravelEloquentSequencer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "ServiceProvider"
            ],
            "aliases": {
                "LaravelEloquentSequencer": "Gurgentil\\LaravelEloquentSequencer\\LaravelEloquentSequencerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
